@model DataStruct.ResultBlob
@{
    ViewBag.Title = "Result File";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section PageCss
{
    
    @*<link href="~/assets/plugins/bower_components/tinymce/skins/lightgray/" rel="stylesheet" type="text/css" />*@
}


@section PageJs
{
    <script src="~/assets/plugins/bower_components/tinymce/jquery.tinymce.min.js"></script>
}

@section PageJsScript
{
    <script>
        tinyMCE.init({
            selector: "textarea#file-editor",
            height: 400,
            setup: (editor) => editor.on('init', () =>
                editor.setContent("")),
        });
    </script>


}


<div class="row bg-title">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h4 class="page-title">LIS Viewer</h4>
    </div>

    <!-- /.col-lg-12 -->
</div>



<div class="row">
    <div class="col-sm-12">
        <div class="white-box">

            <form class="form">
                <div class="row">
                    <div class="col-lg-12">
                        @Html.TextAreaFor(model => model.ResultFile, new { id = "file-editor" })
                    </div>
                </div>
            </form>

            <br />
            <a href="@Request.UrlReferrer" class="btn btn-info">Kembali ke Daftar</a>


        </div>
    </div>

</div>
